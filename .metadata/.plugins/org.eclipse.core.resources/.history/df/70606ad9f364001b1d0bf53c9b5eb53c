<script>
	$(function() {
		//requerst 객체 생성
		xhr = new XMLHttpRequest();

		$("#text").on("click", function() {
			// 요청
			xhr.open("get", "text.jsp", true);
			xhr.send();

			// 응답
			xhr.onreadystatechange = function() {
				if (this.readyState == 4 && this.status == 200) {
					res = this.responseText;
					
				}
			}

		})
	})

</script>
