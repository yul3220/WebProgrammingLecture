<style>
#result1 table{
	text-align: center;
	width : 800px;
}
</style>
<script>
	$(function(){
		$("input").click(function(){
			$.ajax({
				url : "/prod/ProdList",
				type : "get",
				success : function(res){
					$("#result1").html(res);
				},
				error : function(xhr){
					alert("상태 : " + xhr.status);
				},
				dataType : "html"
			})
		})
	})
</script>
</head>
